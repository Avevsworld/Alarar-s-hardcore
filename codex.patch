+
+        Chunk chunk = animals.getLocation().getChunk();
+        long count = chunk.getEntities().length;
+        // TODO: make this radius-based instead of chunk-based.
+        if (count >= maxAnimalsPerChunk) {
+            event.setCancelled(true);
+        }
+    }
+}
